<AnchorPane prefHeight="400.0" prefWidth="711.1" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.photos.fxml.AlbumListController">
   <VBox alignment="TOP_RIGHT" layoutY="2.0" prefHeight="399.0" prefWidth="710.0">
