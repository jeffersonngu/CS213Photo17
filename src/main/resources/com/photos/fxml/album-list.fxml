              <Label fx:id="message" alignment="CENTER" />
         </HBox>
        <ScrollPane fitToWidth="true" prefHeight="323.0" prefWidth="710.0" VBox.vgrow="ALWAYS">
          <FlowPane fx:id="albumFlowPane" hgap="10" prefHeight="315.0" prefWidth="694.0" vgap="10" />
        </ScrollPane>
         <HBox alignment="BOTTOM_LEFT">
              <LogoutButton prefWidth="56.0" text="Logout" />
               <AnchorPane maxWidth="1.7976931348623157E308" prefHeight="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
              <Button mnemonicParsing="false" onAction="#onAddAlbum" prefHeight="40.0" prefWidth="40.0" text="+" />
         </HBox>
   </VBox>
